 RowBox[{"D", "[", 
  RowBox[{
   RowBox[{"V", "[", "p", "]"}], ",", "p"}], "]"}]], "Input",
 CellChangeTimes->{{3.948363851921036*^9, 3.948363856930311*^9}},
 CellLabel->"In[23]:=",ExpressionUUID->"d23a5ec5-6dfe-4e9a-9029-7a362bd6a22e"],

